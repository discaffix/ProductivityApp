<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="ThemeControlForegroundBaseHighBrush" Color="{ThemeResource SystemBaseHighColor}" />

    <Brush x:Key="MainBackgroundColor">#202020</Brush>
    <Brush x:Key="ButtonBackgroundColor">#303030</Brush>
    <Brush x:Key="ItemAndPaneBackgroundColor">#191919</Brush>
    <Brush x:Key="TextColor">#CDCDCD</Brush>

    <Style x:Key="ButtonStyle" TargetType="Control">
        <Setter Property="Background" Value="{StaticResource ButtonBackgroundColor}" />
        <Setter Property="Foreground" Value="{StaticResource TextColor}" />
        <Setter Property="BorderThickness" Value="0" />
        <!--<Setter Property="Margin" Value="{StaticResource SmallTopRightMargin}" />-->
    </Style>

    <Style
        x:Key="AutoSuggestBoxStyle"
        BasedOn="{StaticResource ButtonStyle}"
        TargetType="TextBox" />

</ResourceDictionary>
